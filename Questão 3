#include <stdio.h>

int main() {
    int n, i;

    printf("Digite um numero inteiro positivo: ");
    scanf("%d", &n);

    printf("Fatores primos de %d sao: ", n);

    for(i = 2; i <= n; i++) {
        while(n % i == 0) {
            printf("%d ", i);
            n /= i;
        }
    }

    return 0;
}
